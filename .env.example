# Credentials for the *local mock app* (never a real service)
# These override config.yaml values if set.
SUBSPACE_AUTH_USERNAME=demo
SUBSPACE_AUTH_PASSWORD=demo

# Local-only safety constraint: must remain localhost/127.0.0.1
SUBSPACE_MOCKNET_BASE_URL=http://localhost:8080

# Optional branding
SUBSPACE_MOCKNET__BRAND_NAME=Mock Professional Network

# SQLite database path
SUBSPACE_STORAGE_SQLITE_PATH=data/subspace.db

# Browser automation settings
# - Headless mode is recommended for CI.
# - slow_mo is useful for debugging (e.g. 150ms).
SUBSPACE_BROWSER__HEADLESS=true
SUBSPACE_BROWSER__SLOW_MO=0ms

# Windows note: point Rod at a system-installed browser to avoid downloads/AV issues.
# Examples:
#   SUBSPACE_BROWSER__BIN_PATH=C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe
#   SUBSPACE_BROWSER__BIN_PATH=C:\\Program Files (x86)\\Microsoft\\Edge\\Application\\msedge.exe
SUBSPACE_BROWSER__BIN_PATH=

# Rod's leakless helper can be blocked by AV on some machines.
# Keep false on Windows unless you know leakless works in your environment.
SUBSPACE_BROWSER__LEAKLESS=false

# If true, Rod may download Chromium when no system browser is found.
# Keep false on Windows to avoid AV issues.
SUBSPACE_BROWSER__ALLOW_DOWNLOAD=false

# Optional nested override style (preferred for complex config):
# SUBSPACE_LOGGING__LEVEL=debug
# SUBSPACE_LOGGING__JSON=true
# SUBSPACE_RUN__TIMEOUT=45s
# SUBSPACE_RUN__DETERMINISTIC=true
# SUBSPACE_RUN__SEED=1
